	${MKDIR} -p ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}
	${LINK.cc} -o ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/fasta2tfasta ${OBJECTFILES} ${LDLIBSOPTIONS} -lboost_iostreams

